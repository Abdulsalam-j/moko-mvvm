<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.icerockdev.app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:label="moko-mvvm test app"
        android:theme="@style/Theme.AppCompat.DayNight"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".sample1.SimpleActivity" />
        <activity android:name=".sample2.EventsActivity" />
        <activity android:name=".sample3.EventsOwnerActivity" />
        <activity android:name=".sample4.ValidationMergeActivity" />
        <activity android:name=".sample5.ValidationAllActivity" />
        <activity android:name=".sample6.LoginActivity" />
        <activity android:name=".sample7.ContainerActivity" />
    </application>
</manifest>
